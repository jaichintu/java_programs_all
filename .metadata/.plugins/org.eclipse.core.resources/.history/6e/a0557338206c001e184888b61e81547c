package newlogicsclass;

public class don {
	
	public static void main(String[] args) {
		String str="don";
		
		
		
		for(int i=0; i<str.length(); i++)
		{
			print(str.charAt(i));
		}
				
		
		
	}
	
	
	static void print(char ch)
	{
		char c='a';
		
		while(c<=ch)
		{
			System.out.print(c);
			c=(char)(c+1);
		}
		
	}

}

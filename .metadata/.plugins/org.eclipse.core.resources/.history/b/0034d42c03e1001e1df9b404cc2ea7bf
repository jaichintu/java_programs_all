package interviewPractice;

import java.util.Arrays;

public class Rotation {
	public static void main(String[] args) {
		int [] arr={1,2,3,4,5,6,7,8};
		
		int k=2;
		int  n=arr.length-1;
		int index=0;
		for(int i=0; i<k; i++)
		{
			int temp=arr[index];
			index++;
			for(int j=0; j<n; j++){
				arr[temp]=arr[j+1];
			}
			arr[k]=temp;
		}
		System.out.println(Arrays.toString(arr));
	}

}

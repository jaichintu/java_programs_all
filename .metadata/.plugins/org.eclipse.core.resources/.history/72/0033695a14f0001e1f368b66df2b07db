package onlypatterns;

public class fibnoccistarpattern {
    public static void main(String[] args) {
        int n = 10;
        int fn = 0;
        int sn = 1;
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < fn; j++) {
                System.out.print("*");
                
            }
            System.out.println();
            
            int nextnum = fn + sn;
            fn = sn;
            sn = nextnum;
        }
    }
}

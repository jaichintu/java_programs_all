package testfresh;

public class newarray {
	static void sumtillsingledigit(int [] arr)
	{
		int sum=0;
		for (int i = 0; i < arr.length; i++) {
			if(arr[i]>=0 && arr[i]<=9)
			{
				sum=sum+arr[i];
			}
			System.out.println(sum);
		}
	}
	public static void main(String[] args) {
		int [] arr ={13,2,32,4,5};
		sumtillsingledigit(arr);
	}

}

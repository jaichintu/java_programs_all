package programs;

public class primeperfect {
	public static void main(String[] args) {
		int no=28;
		int sum = 0;
		for(int i=2; i<=no/2;i++)
		{
			if(no%i==0 && prime(i))
			{
				System.out.println(i);
			}
		}
		
	}

	 static boolean prime(int no) {
		 for(int i=2; i<=no/2; i++)
		 {
			 if(no%i==0)
			 {
				 return false; 
			 }
		 }
		// TODO Auto-generated method stub
		return false;
		
	}

}

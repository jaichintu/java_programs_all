package onlypatterns;

public class fibnoccistarpattern {
	
	public static void main(String[] args) {
		int n=5;
		int fn=0;
		int sn=1;
		for(int i=1; i<=n; i++)
		{
			for(int j=1; j<=i; j++)
			{
				System.out.print("*");
				int nextnum=fn+sn;
				fn=sn;
				sn=nextnum;
			}
//			int fibnum=fn;
//			for(int k=1; k<=fibnum; k++)
//			{
//				System.out.print("*");
//			}
			
			System.out.println();
		}
		
	}

}
